{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"description": "Auto-Generated Release Please configuration for prerelease",
	"pull-request-header": "chore: :robot: prerelease release prepared",
	"release-type": "node",
	"versioning": "prerelease",
	"prerelease": true,
	"prerelease-type": "next",
	"separate-pull-requests": false,
	"packages": {
		"packages/cli-core": {
			"package-name": "cli-core",
			"changelog-path": "docs/changelog.md"
		},
		"packages/core": {
			"package-name": "core",
			"changelog-path": "docs/changelog.md"
		},
		"packages/crypto": {
			"package-name": "crypto",
			"changelog-path": "docs/changelog.md"
		},
		"packages/entity": {
			"package-name": "entity",
			"changelog-path": "docs/changelog.md"
		},
		"packages/image": {
			"package-name": "image",
			"changelog-path": "docs/changelog.md"
		},
		"packages/modules": {
			"package-name": "modules",
			"changelog-path": "docs/changelog.md"
		},
		"packages/qr": {
			"package-name": "qr",
			"changelog-path": "docs/changelog.md"
		},
		"packages/web": {
			"package-name": "web",
			"changelog-path": "docs/changelog.md"
		},
		"apps/crypto-cli": {
			"package-name": "crypto-cli",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/version.ts"]
		}
	},
	"plugins": [
		"node-workspace",
		{
			"type": "linked-versions",
			"groupName": "repo",
			"components": [
				"cli-core",
				"core",
				"crypto",
				"entity",
				"image",
				"modules",
				"qr",
				"web",
				"crypto-cli"
			]
		}
	]
}
