{
	"language": "en-GB",
	"ignorePaths": [
		"**/node_modules/**",
		"LICENSE",
		"**/coverage/**",
		"**/dist/**",
		"**/docs/reference/**",
		"**/docs/open-api/**",
		"**/tests/**",
		"*.yaml",
		"package.json",
		"rollup.config.mjs",
		"vitest.config.*",
		"tsconfig.json",
		"typedoc.json",
		".eslintrc.cjs",
		"**/wordlists/**"
	],
	"words": [
		"AARRGGBB",
		"ARGB",
		"Bech",
		"chacha",
		"decryptor",
		"encryptor",
		"Hmac",
		"Hotp",
		"jsonld",
		"nameof",
		"otpauth",
		"paeth",
		"Plte",
		"Priv",
		"Quant",
		"RGBYUV",
		"RRGGBB",
		"Secp",
		"Stiftung",
		"Totp",
		"tspc",
		"ttsc",
		"ttypescript",
		"UVQT"
	],
	"ignoreRegExpList": ["/from\\s+(['\"]).*\\1/", "/eslint-disable.*/"]
}
